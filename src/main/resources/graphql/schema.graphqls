scalar DateTime

type Query{
    getAllTodos : [Todos]
}

enum Priority{
    NONE,
    LOW,
    MEDIUM,
    HIGH
}

type Todos {
    _id: ID!
    task: String!
    notes: String
    priority: Priority!
    done: Boolean!
    closed: Boolean!
    createdAt: DateTime
    dueDate: DateTime
}

type Mutation{
    addTodo(task:String!):Todos
}